$(".n-main-nav li").each(function(c,e){var d=$(e);var g=location.pathname;var f=g.substring(11,g.length-5);if(!d.hasClass("current")){var b=d.find("a");b.attr("href","/pass/auth/"+b.data("action")+"/home"+location.search)}if(f===b.data("action")){d.addClass("current")}});$(".nav-item").each(function(){$(this).attr("href","/pass/auth/"+$(this).data("action")+"/home"+location.search)});if(JSP_VAR.deviceType==="mobile"){if(getParam("_hideLogout")==="true"){$(".log-out, .n-footer").hide()}}function getIpv6Adress(){$.ajax({type:"GET",url:"/pass/ip/next",dataType:"text",success:function(c){var b=$.parseJSON(c.replace("&&&START&&&",""));if(b.code===0){if(b.data.hasNextUrl===true){var a=document.createElement("img");a.src=b.data.url;document.body.appendChild(a);setTimeout(function(){document.body.removeChild(a)},5000)}}}})}if(!getCookie("ipAddress")&&JSP_VAR.dataCenterZone==="China"){getIpv6Adress()}function getCookie(d){var c=document.cookie;var a=c.split(";");var e="";for(var b=0;b<a.length;b++){if($.trim(a[b]).substr(0,d.length)==d){e=$.trim(a[b]).substr(d.length+1);break}}return unescape(e)};